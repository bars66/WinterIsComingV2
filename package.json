{
  "name": "winteriscomingv2",
  "version": "1.0.0",
  "private": true,
  "main": "packages/bridge/src/index.ts",
  "repository": "git@github.com:bars66/WinterIsComingV2.git",
  "author": "Nikolay Pavlushin <i@bars66.com>",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "babel": "yarn workspace winteriscomingv2-bridge babel & yarn workspace winteriscomingv2-common babel",
    "babel-watch": "echo 'Watch mode' && babel src --watch --verbose --extensions \".js,.jsx,.ts,.tsx\" --skip-initial-build --out-dir build",
    "dev-mode": "yarn workspace winteriscomingv2-bridge dev-mode & yarn workspace winteriscomingv2-common dev-mode",
    "app-watch": "yarn dev-mode & nodemon build/index | bunyan",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "css-loader": "1.0.1",
    "eslint-config-standard": "^14.1.1",
    "husky": "^1.3.1",
    "nodemon": "^1.19.4",
    "standard": "^14.3.4",
    "style-loader": "1.2.1",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
